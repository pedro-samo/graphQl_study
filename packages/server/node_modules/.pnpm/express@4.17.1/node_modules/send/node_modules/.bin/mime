    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/node_modules:/mnt/c/Users/pedro/Desktop/Developer/node_modules:/mnt/c/Users/pedro/Desktop/node_modules:/mnt/c/Users/pedro/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/mime/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/send@0.17.1/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/send/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/node_modules:/mnt/c/Users/pedro/Desktop/Developer/node_modules:/mnt/c/Users/pedro/Desktop/node_modules:/mnt/c/Users/pedro/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/mime/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/send@0.17.1/node_modules:/mnt/c/Users/pedro/Desktop/Developer/Hiring Coders/graphQl_study/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/send/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../mime/cli.js" "$@"
else
  exec node  "$basedir/../../../mime/cli.js" "$@"
